{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "My personal website.",
  "scripts": {
    "compile-pug": "pug --out docs --pretty src/pug -O src/data.js",
    "compile-sass": "node-sass --output docs/css/ src/sass/",
    "compile-ts": "rollup -c",
    "watch-pug": "npm run compile-pug -- -w",
    "watch-sass": "npm run compile-sass -- -w -r",
    "watch-ts": "npm run compile-ts -- -w",
    "serve": "http-server ./docs --silent -c-1",
    "dev": "npm-run-all --parallel watch-pug watch-sass watch-ts serve",
    "build": "npm-run-all compile-pug compile-sass compile-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tannerntannern@bitbucket.org/tannerntannern/portfolio-website.git"
  },
  "author": "Tanner Nielsen",
  "license": "ISC",
  "homepage": "https://bitbucket.org/tannerntannern/portfolio-website#readme",
  "devDependencies": {
    "http-server": "^0.10.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^0.52.3",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {}
}
