{
  "name": "portfolio-website",
  "version": "2.0.0",
  "description": "My personal website.",
  "scripts": {
    "build-clean": "rm docs/*.css docs/*.html docs/*.js && npm run build && git add docs/*.css docs/*.html docs/*.js",
    "build": "parcel build src/pug/* --out-dir docs --no-source-maps --public-url ./",
    "watch": "parcel watch src/pug/* --out-dir docs --no-source-maps --no-hmr --public-url ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-clean"
    }
  },
  "dependencies": {
    "regl": "^1.3.11"
  },
  "devDependencies": {
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "husky": "^1.2.1",
    "parcel-bundler": "^1.11.0",
    "pug": "^2.0.3",
    "sass": "^1.15.2",
    "typescript": "^3.2.2"
  },
  "author": "Tanner Nielsen"
}
