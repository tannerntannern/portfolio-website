{
  "name": "website",
  "version": "1.0.0",
  "description": "My personal website.",
  "scripts": {
    "clean": "rm docs/*.html docs/*.css docs/*.js && rm -rf docs/__",
    "watch": "parcel watch src/pug/*.pug --public-url . -d docs --no-source-maps --no-hmr",
    "build": "parcel build src/pug/*.pug --public-url . -d docs --no-source-maps",
    "build-clean": "yarn clean && yarn build && git add docs"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^1.3.1",
    "parcel-bundler": "^1.11.0",
    "pug": "^2.0.3",
    "sass": "^1.16.1",
    "typescript": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tannerntannern@bitbucket.org/tannerntannern/website.git"
  },
  "homepage": "https://tannernielsen.com",
  "author": "Tanner Nielsen",
  "license": "ISC"
}
